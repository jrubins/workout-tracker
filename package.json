{
  "name": "workout-tracker",
  "version": "1.0.0",
  "description": "A workout tracker.",
  "author": "Jon Rubins <jonrubins@gmail.com>",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --config webpack.config.prod.js",
    "precommit": "lint-staged && yarn test",
    "start": "git pull && yarn start:merge",
    "start:merge": "yarn && yarn start:offline",
    "start:debug": "DEBUG=true && yarn start",
    "start:offline": "webpack-dev-server --progress --config webpack.config.dev.js",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": "8.11.3",
    "npm": "5.6.0",
    "yarn": "1.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jrubins/workout-tracker.git"
  },
  "license": "MIT",
  "jest": {
    "cacheDirectory": "<rootDir>/.jest-cache",
    "clearMocks": true,
    "resetMocks": true,
    "resetModules": true,
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "<rootDir>/src/utils/tests/setup/"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix --cache --ext=.js,.jsx",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{scss}": [
      "sass-lint -v -q"
    ]
  },
  "dependencies": {
    "classnames": "2.2.6",
    "jwt-decode": "2.2.0",
    "lodash": "4.17.10",
    "moment": "2.22.2",
    "normalize.css": "8.0.0",
    "object-hash": "1.3.0",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "recharts": "1.1.0",
    "redux": "4.0.0",
    "redux-localstorage": "0.4.1",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "9.1.3",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "enzyme": "3.4.4",
    "enzyme-adapter-react-16": "1.2.0",
    "eslint": "5.4.0",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "jest": "23.5.0",
    "lint-staged": "7.2.2",
    "mini-css-extract-plugin": "0.4.2",
    "nock": "9.6.1",
    "node-sass": "4.9.3",
    "postcss-loader": "3.0.0",
    "prettier": "1.14.2",
    "react-hot-loader": "4.0.0",
    "sass-lint": "1.12.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.22.1",
    "webpack": "4.17.1",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5",
    "webpack-dotenv-plugin": "2.1.0"
  }
}
